{
  "name": "ololos-blog-react-redux-universal",
  "description": "Ololos-blog is a travel blog of me and my sweet girlfriend",
  "author": "Andrii Los <puha212@gmail.com> (http://github.com/rip21)",
  "license": "MIT",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/RIP21/ololos-blog-react-redux-universal"
  },
  "main": "bin/server.js",
  "scripts": {
    "start": "concurrently -r --kill-others \"npm run start-prod\"",
    "start-prod": "better-npm-run start-prod --color",
    "build": "better-npm-run build --color",
    "postinstall": "npm run build",
    "lint": "eslint -c .eslintrc src",
    "lint:fix": "eslint --fix -c .eslintrc src",
    "start-dev": "better-npm-run start-dev --color -s",
    "watch-client": "better-npm-run watch-client --color -s",
    "dev": "concurrently -r --kill-others \"npm run watch-client\" \"npm run start-dev\"",
    "test": "karma start"
  },
  "betterScripts": {
    "start-prod": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 3000,
        "APIPORT": 8080
      }
    },
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000,
        "APIPORT": 8080
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src",
        "PORT": 3000,
        "APIPORT": 8080
      }
    },
    "build": {
      "command": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
