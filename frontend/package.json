{
  "name": "Ololos-blog-react-universal",
  "description": "Ololos travel blog",
  "author": "Andrii Los <puha212@gmail.com>",
  "license": "MIT",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/RIP21/ololos-blog-react-redux-universal"
  },
  "homepage": "http://ololos.space/",
  "keywords": [
    "react",
    "react-router",
    "react-redux",
    "redux",
    "universal",
    "isomorphic",
    "webpack",
    "express",
    "starter-kit",
    "boilerplate",
    "babel"
  ],
  "main": "bin/server.js",
  "scripts": {
    "clean": "node ./bin/clean.js",
    "start": "nodemon ./bin/server.js",
    "prebuild": "npm-run-all clean",
    "build": "npm-run-all --parallel build:*",
    "build:server": "webpack --config webpack/server/config.js",
    "build:client": "webpack --config webpack/client/config.js",
    "deploy": "cross-env NODE_ENV=production npm-run-all build start",
    "prewatch": "npm run clean",
    "watch": "npm-run-all --parallel watch:*",
    "watch:wait": "just-wait --pattern \"dist/*.js\" && npm run start",
    "watch:server": "webpack --config webpack/server/watch.js",
    "watch:client": "webpack-dev-server --config webpack/client/watch.js",
    "lint": "eslint ./src ./bin ./webpack ./tests -c ./.eslintrc",
    "prettier": "prettier --write --print-width 120 {src,tests,webpack}/**/*.js"
  },
  "dependencies": {
    "compression": "latest",
    "react-bootstrap": "latest",
    "express": "^4.15.2",
    "http-proxy": "latest",
    "locale": "^0.1.0",
    "lodash": "^4.17.4",
    "moment": "latest",
    "node-noop": "^1.0.0",
    "node-sass": "^4.5.0",
    "object-assign": "latest",
    "react": "^15.4.2",
    "react-disqus-comments": "0.5.2",
    "react-dom": "^15.4.2",
    "react-ga": "latest",
    "react-helmet": "^4.0.0",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "react-simplemde-editor": "latest",
    "react-tap-event-plugin": "^2.0.1",
    "redial": "^0.5.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "remarkable": "latest",
    "reselect": "latest",
    "sanitize.css": "^5.0.0",
    "sass-loader": "^6.0.3",
    "semantic-ui-css": "^2.2.9",
    "serialize-javascript": "^1.3.0",
    "serve-favicon": "^2.4.3",
    "style-loader": "^0.13.2",
    "styled-components": "2.0.0-17",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "superagent": "^3.5.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.5.1",
    "webpack-isomorphic-tools": "^3.0.1"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.4.0",
    "babel-plugin-styled-components": "^1.1.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.22.0",
    "body-parser": "^1.17.1",
    "chai": "^3.5.0",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.4",
    "del": "^2.2.2",
    "enzyme": "^2.7.1",
    "eslint": "^3.17.1",
    "eslint-config-google": "^0.7.1",
    "eslint-plugin-react": "^6.10.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "json-loader": "^0.5.4",
    "just-wait": "^1.0.11",
    "npm-run-all": "^4.0.2",
    "prettier": "^1.3.1",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "sinon": "^1.17.7",
    "webpack-dev-server": "^2.4.5",
    "webpack-node-externals": "^1.4.3",
    "nodemon": "^1.11.0"
  },
  "engines": {
    "node": ">=6.9.0"
  }
}
